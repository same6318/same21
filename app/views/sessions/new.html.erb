<h1><%= t(".title") %></h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class="field">
    <%= f.label :email, I18n.t("activerecord.attributes.user.email") %>
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :password, I18n.t("activerecord.attributes.user.password") %>
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%#= f.submit t("create"), id: "create-session" %>
    <%= f.submit "ログイン", id: "create-session" %>
  </div>
<% end %>